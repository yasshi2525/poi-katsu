{
	"width": 1280,
	"height": 720,
	"fps": 60,
	"main": "./script/_bootstrap.js",
	"assets": {
		"player": {
			"type": "image",
			"width": 32,
			"height": 32,
			"path": "image/player.png"
		},
		"shot": {
			"type": "image",
			"width": 16,
			"height": 16,
			"path": "image/shot.png"
		},
		"se": {
			"type": "audio",
			"path": "audio/se",
			"systemId": "sound",
			"duration": 250,
			"hint": {
				"extensions": [
					".aac",
					".ogg"
				]
			}
		},
		"_bootstrap": {
			"type": "script",
			"path": "script/_bootstrap.js",
			"global": true
		},
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"parameterObject": {
			"type": "script",
			"path": "script/parameterObject.js",
			"global": true
		}
	},
	"environment": {
		"sandbox-runtime": "3",
		"nicolive": {
			"supportedModes": [
				"ranking"
			],
			"preferredSessionParameters": {
				"totalTimeLimit": 82
			}
		}
	},
	"globalScripts": []
}